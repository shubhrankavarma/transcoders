    "definitions": {
        "handlers.StatusType": {
            "type": "string",
            "enum": [
                "active",
                "inactive"
            ],
            "x-enum-varnames": [
                "Active",
                "Inactive"
            ]
        },
        "handlers.Transcoder": {
            "type": "object",
            "required": [
                "asset_type",
                "operation",
                "template_command",
                "updated_by"
            ],
            "properties": {
                "asset_type": {
                    "type": "string",
                    "example": "Audio, Video, Image, Text"
                },
                "audio_count": {
                    "description": "Audio extraction fields",
                    "type": "integer",
                    "example": 1
                },
                "channels_one_count": {
                    "type": "integer",
                    "example": 1
                },
                "channels_six_count": {
                    "type": "integer",
                    "example": 1
                },
                "channels_two_count": {
                    "type": "integer",
                    "example": 1
                },
                "created_at": {
                    "type": "string"
                },
                "description": {
                    "type": "string",
                    "example": "media_analysis"
                },
                "id": {
                    "description": "To be used as a primary key and mandatory field",
                    "type": "string"
                },
                "input_scan_type": {
                    "description": "Video extraction fields",
                    "type": "string",
                    "example": "progressive"
                },
                "input_type": {
                    "type": "string",
                    "example": "mp4"
                },
                "operation": {
                    "description": "Operation String - required | examples - Media Analysis, Encoding, Packaging, Processing, Extraction, QC, Sticking, Spliting",
                    "type": "string",
                    "example": "media_analysis"
                },
                "output_scan_type": {
                    "type": "string",
                    "example": "interlaced"
                },
                "output_type": {
                    "description": "Types of input and output",
                    "type": "string",
                    "example": "dash"
                },
                "status": {
                    "allOf": [
                        {
                            "$ref": "#/definitions/handlers.StatusType"
                        }
                    ],
                    "example": "active"
                },
                "template_command": {
                    "description": "Default Value is \"Coming Soon\"",
                    "type": "string",
                    "example": "coming soon"
                },
                "updated_at": {
                    "type": "string"
                },
                "updated_by": {
                    "type": "string",
                    "example": "me"
                }
            }
        }
